{
  "name": "feishu2html",
  "version": "1.0.0",
  "description": "Convert Feishu (Lark) documents to beautiful, standalone HTML files - Kotlin Multiplatform library and CLI tool",
  "type": "module",
  "main": "./build/js/packages/feishu2html/kotlin/feishu2html.mjs",
  "bin": {
    "feishu2html": "./bin/feishu2html.mjs"
  },
  "files": [
    "bin/",
    "build/js/packages/feishu2html/kotlin/",
    "build/compileSync/js/main/productionExecutable/kotlin/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "./gradlew compileKotlinJs",
    "prepublishOnly": "npm run build",
    "test:js": "node test-js.mjs",
    "test:example": "node test-js-example.mjs",
    "test": "npm run build && npm run test:js"
  },
  "keywords": [
    "feishu",
    "lark",
    "html",
    "converter",
    "cli",
    "kotlin-js",
    "multiplatform",
    "document-converter"
  ],
  "author": "YidaFu <yidafu90@qq.com>",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yidafu/feishu2html.git"
  },
  "homepage": "https://github.com/yidafu/feishu2html#readme",
  "bugs": {
    "url": "https://github.com/yidafu/feishu2html/issues"
  },
  "dependencies": {
    "abort-controller": "^3.0.0",
    "node-fetch": "^3.3.2"
  }
}
